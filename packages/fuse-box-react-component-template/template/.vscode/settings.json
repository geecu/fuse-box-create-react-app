// Place your settings in this file to overwrite default and user settings.
{
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/node_modules/**": true,
        "**/bower_components": true,
        "**/bower_components_app": true,
        "**/public/css/raw": true,
        "**/public/js/app": true,
        "**/.fusebox/**": true
    },
    "search.exclude": {
        "**/public/css/raw": true,
        "**/public/js/app": true,
        "**/node_modules": true,
        "**/bower_components": true,
        "**/bower_components_app": true,
        "**/build/**": true,
        "**/public/js/common": true,
        "**/public/js/vendor": true,
        "**/.fusebox/**": true
    }
}